@page
@model PyImpresoras.Frontend.Pages.CrearTipoRevisionModel
@{
     ViewData["Title"] = "Tipo de revision";
}
<h1>Registro del tipo de revisiones en el sistema</h1>
<form method="post">
  <div class="mb-3">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <label asp-for="TipoRevision.TipoDeRevision" class="form-label"> Tipo de revision</label>
    <input type="text" asp-for="TipoRevision.TipoDeRevision"  class="form-control" id="Tipo" aria-describedby="emailHelp">
    <span asp-validation-for="TipoRevision.TipoDeRevision" class="text-danger"></span>
  </div>
  <button type="submit" class="btn btn-primary">Registrar tipo revision</button>
</form>